{
  "name": "@bonfhir/campfhir-cli",
  "description": "Campfhir CLI",
  "version": "0.0.0",
  "repository": "https://github.com/bonfhir/campfhir.git",
  "license": "APACHE-2.0",
  "type": "module",
  "bin": {
    "campfhir": "index.js"
  },
  "scripts": {
    "build": "yarn clean && tsup src/index.ts --format esm,cjs --out-dir dist/ --shims --dts --tsconfig tsconfig.build.json",
    "check": "prettier --check ./src && eslint ./src && tsc --noEmit",
    "clean": "rimraf dist/",
    "dev": "ts-node --esm --swc --transpileOnly --experimental-specifier-resolution=node src/index.ts",
    "format": "prettier --loglevel warn --write ./**/*.ts && eslint --fix ./**/*.ts",
    "package:create": "yarn build && node package.js pack",
    "package:publish": "yarn build && node package.js publish",
    "test": "jest"
  },
  "packageManager": "yarn@3.3.1",
  "devDependencies": {
    "@bonfhir/eslint-config": "^1.1.0-alpha.0",
    "@bonfhir/prettier-config": "^1.0.0",
    "@bonfhir/typescript-config": "^1.0.1-alpha.0",
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.24",
    "@swc/jest": "^0.2.24",
    "@types/fhir": "^0.0.35",
    "@types/jest": "^29.2.4",
    "@types/listr": "^0",
    "@types/lodash": "^4",
    "@types/node": "^18.11.18",
    "@types/prompts": "^2",
    "@types/rimraf": "^3",
    "@types/yargs": "^17",
    "eslint": "^8.30.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsup": "^6.6.2",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@bonfhir/core": "^1.0.0-alpha.10",
    "@bonfhir/medplum": "^1.0.0-alpha.11",
    "@medplum/core": "^2.0.5",
    "chalk": "^5.2.0",
    "dotenv": "^16.0.3",
    "fast-glob": "^3.2.12",
    "langchain": "^0.0.44",
    "listr": "^0.14.3",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.1",
    "prompts": "^2.4.2",
    "yargs": "^17.6.2"
  },
  "prettier": "@bonfhir/prettier-config"
}
